<%@ Master Language="C#" AutoEventWireup="true" CodeBehind="Site1.master.cs" 
    Inherits="WebApplication1.Site1" %>

<!DOCTYPE html>

<html>
<head runat="server">
    <title></title>

    <%--bootstrap css--%>
    <link href="Bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <%--datatables css--%>
    <link href="DataTables/css/jquery.dataTables.min.css" rel="stylesheet" />
    <%--fontawesome css--%>
    <link href="FontAwesome/css/all.css" rel="stylesheet" />
    <%--our custom css--%>
    <link href="css/customstylesheet.css" rel="stylesheet" />

    <%--jquery--%>
    <script src="Bootstrap/js/jquery-3.3.1.slim.min.js"></script>
    <%--popper js--%>
    <script src="Bootstrap/js/popper.min.js"></script>
    <%--bootstrap js--%>
    <script src="Bootstrap/js/bootstrap.min.js"></script>

    <%--jquery--%>
    <script src="Scripts/jquery-3.5.1.min.js"></script>
    <script src="Scripts/app.js"></script>

    <asp:ContentPlaceHolder ID="head" runat="server">
    </asp:ContentPlaceHolder>
</head>
<body>
    <form id="form1" runat="server">

        <div>
            <nav class="navbar navbar-expand-md navbar-light bg-light sticky-top">
                <a class="navbar-brand" href="homepage.aspx">
                    <img src="Images/first-aid.png" width="30" height="30" />
                    E-Hospital
                </a>

                <button class="navbar-toggler" type="button" 
                    data-toggle="collapse"
                    data-target="#navbarSupportedContent"
                    aria-controls="navbarSupportedContent" 
                    aria-expanded="false" 
                    aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    <ul class="navbar-nav ml-auto">
                        <li class="nav-item active"">
                            <asp:LinkButton class="nav-link" ID="LinkButton1" runat="server" OnClick="LinkButton1_Click">Log In</asp:LinkButton>
                        </li>
                        <li class="nav-item active">
                            <asp:LinkButton class="nav-link btn btn-outline-dark px-2 py-2" ID="LinkButton2" runat="server" OnClick="LinkButton2_Click">Sign Up</asp:LinkButton>
                        </li>
                        <li class="nav-item active" id="userpanel">
                            <asp:HyperLink class="nav-link" ID="HyperLink2" runat="server" Visible="False">User Panel <i class="fa fa-angle-down"></i></asp:HyperLink>
                            <ul>
                                <li>
                                    <asp:LinkButton class="nav-link" ID="LinkButton7" runat="server" OnClick="LinkButton7_Click" Visible="True">View Profile</asp:LinkButton>
                                </li>
                            </ul>
                        </li>
                        <li class="nav-item active" id="medicpanel">
                            <asp:HyperLink class="nav-link" ID="HyperLink1" runat="server" Visible="False">Medic Panel <i class="fa fa-angle-down"></i></asp:HyperLink>
                            <ul>
                                <li>
                                    <asp:LinkButton class="nav-link" ID="LinkButton5" runat="server" OnClick="LinkButton5_Click" Visible="True">Patients List</asp:LinkButton>
                                </li>
                            </ul>
                        </li>
                        <li class="nav-item active" id="medicpanel">
                            <asp:HyperLink class="nav-link" ID="HyperLink3" runat="server" Visible="False">Secretary Panel <i class="fa fa-angle-down"></i></asp:HyperLink>
                            <ul>
                                <li>
                                    <asp:LinkButton class="nav-link" ID="LinkButton4" runat="server" OnClick="LinkButton4_Click" Visible="True">Patients List</asp:LinkButton>
                                </li>
                            </ul>
                        </li>
                        <li class="nav-item active">
                            <asp:LinkButton class="nav-link" ID="LinkButton3" runat="server" Visible="False" OnClick="LinkButton3_Click">Log Out</asp:LinkButton>
                        </li>
                    </ul>
                </div>
            </nav>
        </div>

        <!-- Main Content Placeholder -->
        <div>
            <asp:ContentPlaceHolder ID="ContentPlaceHolder1" runat="server">
            </asp:ContentPlaceHolder>
        </div>
        <!-- Main Content Placeholder -->

        <!-- Footer -->
        <footer>

           <div class="container-fluid padding">
               <div class="row text-center">
                   <div class="col-md-12" id="Staff" runat="server">
                       <h5>Staff Connect</h5>
                       <hr class="light">
                       <p>
                            <asp:LinkButton class="footerlinks" ID="LinkButton6" runat="server" OnClick="LinkButton6_Click">Admin Login</asp:LinkButton>
                            &nbsp;
                            <asp:LinkButton class="footerlinks" ID="LinkButton11" runat="server" OnClick="LinkButton11_Click">Medic Login</asp:LinkButton>
                            &nbsp;
                            <asp:LinkButton class="footerlinks" ID="LinkButton12" runat="server" OnClick="LinkButton12_Click">Secretary Login</asp:LinkButton>
                            &nbsp;
                        </p
                    </div>
                   
                   <div class="col-md-12">
                       <p>
                           <asp:Panel ID="Panel1" Visible="false" runat="server"><asp:Label ID="LabelAboutUsHeader" runat="server"></asp:Label></asp:Panel>
                       </p>
                   </div>

                    <div class="col-12">
                        <hr class="light">
                        <h6>&copy; E-Hospital</h6>
                    </div>
           </div>
           </div>

        </footer>
        <!-- ./Footer -->

    </form>
</body>
</html>
